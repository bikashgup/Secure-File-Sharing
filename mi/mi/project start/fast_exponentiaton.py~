import sys

def f_expo(data,key,n):
    temp = bin(key)[2:]
    length= len(temp)
    if key==0:
        return 1
    result=data
    for i in range(len(temp)-1,0):
        if(temp[i]==0):
            result=(result**2)%n
        else:
            result=(result**2 *data)%n
    return result
    
        
            
            
        
    
